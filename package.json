{
  "name": "ad-doctor-app",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve_before": "cross-env NODE_ENV=development vue-cli-service build --color --mode test",
    "serve": "cross-env NODE_OPTIONS=--max_old_space_size=8192 vue-cli-service serve --color",
    "test": "vue-cli-service build --color --mode test",
    "build": "vue-cli-service build --color",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "core-js": "^2.6.5",
    "mescroll.js": "^1.4.1",
    "qs": "^6.9.0",
    "tree-node-cli": "^1.4.0",
    "vue": "^2.6.10",
    "vuex": "^3.5.1",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.1",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-plugin": "^4.3.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^7.0.0",
    "postcss-px2rem": "^0.3.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px2rem": {
        "remUnit": 37.5
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "not ie <= 11",
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "resolutions": {
    "html-webpack-plugin": "^4.3.0"
  }
}
